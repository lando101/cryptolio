{
  "name": "ng-x-rocket",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "ng serve --host 0.0.0.0",
    "serve:sw": "npm run build -s && npx http-server ./dist/starter-kit-master -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "generate": "ng generate",
    "aws-deploy": "aws s3 sync dist/starter-kit-master/ s3://cryptolio-containerized"
  },
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "^9.1.9",
    "@angular/localize": "^9.1.0",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@fortawesome/fontawesome-free": "^5.1.0",
    "@ionic/angular": "^5.2.3",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "@ngx-translate/core": "^12.0.0",
    "@types/jquery": "^3.5.0",
    "angular-particle": "^1.0.4",
    "bootstrap": "^4.1.1",
    "bootstrap-icons": "^1.0.0-alpha5",
    "chart.js": "^2.9.3",
    "coinmarketcap-api": "^3.1.1",
    "firebase": "^7.15.5",
    "lightweight-charts": "^3.1.1",
    "ng2-charts": "^2.3.2",
    "ng2-search-filter": "^0.5.1",
    "ngx-bootstrap": "^5.6.1",
    "ngx-tweet": "^2.0.3",
    "ngx-twitter-timeline": "^0.1.5",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^4.2.0",
    "@ngx-rocket/scripts": "^4.0.0",
    "@schematics/angular": "^10.0.0",
    "@types/chart.js": "^2.9.22",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^25.1.2",
    "@types/node": "^12.12.48",
    "codelyzer": "^5.1.2",
    "hads": "^2.0.1",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^5.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "stylelint": "~13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended-scss": "~4.2.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.16.0",
    "ts-node": "^8.8.1",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.8.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
